Xft.dpi:                                       96 
Xft.antialias:                                  true
Xft.rgba:                                       rgb
Xft.hinting:                                    true
Xft.hintstyle:                                  hintslight

URxvt.url-launcher:      /usr/bin/firefox
URxvt.matcher.button:   1


URxvt.depth: 32
URxvt.saveLines: 12000
!URxvt.background: [85]#000000

URxvt*geometry:                                 80x25
URxvt*borderLess:                               false
URxvt*scrollBar:                                false
URxvt*cursorBlink:                              true
URxvt*cursorUnderline:                          false
URxvt*scrollColor:                              #151515
URxvt*font: xft:Inconsolata:style=Regular:pixelsize=14:antialias=true
URxvt.underlineURLs:                            true
URxvt.url-select.launcher:                      /usr/bin/firefox
URxvt.url-select.underline:                     true
URxvt*keysym.Home: \033[1~
URxvt*keysym.End: \033[4~

*cursorColor:                         #17F018
*VT100.colorBDMode:                   true
*VT100.colorULMode:                   true
*VT100.colorUL:                       magenta
*VT100.veryBoldColors:                6

*background: [80]#000000
*foreground: #17F018

URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard
